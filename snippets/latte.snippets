snippet foreach
	{foreach $${1} as $${2}}
		${3}
	{/foreach}
snippet dump
	{dump $${1}}
snippet var
	{var $${1} = ${2}}
snippet _
	{_"${1}"}
snippet if
	{if $${1}}
		${2}
	{/if}
snippet else
	{else}
snippet elseif
	{elseif $${1}}
snippet eif
	{elseif $${1}}
snippet fa
	<span class="fa fa-fw fa-${1}"></span>
